[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	uncommit = reset --soft HEAD~1
	hist = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold blue)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	graph = log --oneline --decorate --graph --all
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
[user]
	name = Emerson Pfeiffer
	email = epfeiffe@umich.edu

[color]
	ui = auto
[color "branch"]
	current = green bold
	local = white
	remote = blue
[color "diff"]
	meta = magenta bold 
	frag = cyan bold 
	old = red bold
	new = green bold
[color "status"]
	added = green bold
	changed = red bold
	untracked = red bold

